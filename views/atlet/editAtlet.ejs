<!-- Modal -->
<form action="/atlet/edit" method="POST" enctype="multipart/form-data">
    <div class="modal fade" id="editAtlet" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h1 class="modal-title fs-5" id="exampleModalLabel">Edit Atlet</h1>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <input type="submit" name="id_atlet" class="product_id2" value="<%= atlet.id_atlet %>">
                    <%= atlet.id_atlet %>
                    <div class="row">
                        <div class="form-group col-md mb-3">
                            <label for="nama" class="form-label">Nama</label>
                            <input type="text" class="form-control nama" id="nama" name="nama"  >
                        </div>
                        <div class="form-group col-md mb-3">
                            <label for="tempat_lahir" class="form-label">Tempat Lahir</label>
                            <input type="text" class="form-control tempat_lahir" id="tempat_lahir" name="tempat_lahir"  >
                        </div>
                        <div class="form-group col-md mb-3">
                            <label for="tanggal_lahir" class="form-label">Tanggal Lahir</label>
                            <input type="date" class="form-control tanggal_lahir" id="tanggal_lahir" name="tanggal_lahir"  >
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md mb-3">
                            <label for="jenis_kelamin" class="form-label">Jenis Kelamin</label>
                            <select type="text" class="form-select jenis_kelamin" id="jenis_kelamin" name="jenis_kelamin"  >
                                <option selected>Jenis Kelamin</option>
                                <option value="Laki-laki">Laki-laki</option>
                                <option value="Perempuan">Perempuan</option>
                            </select>
                        </div>
                        <div class="form-group col-md mb-3">
                            <label for="no_telp" class="form-label">No Telp</label>
                            <input type="text" class="form-control no_telp" id="no_telp" name="no_telp"  >
                        </div>
                        <div class="form-group col-md mb-3">
                            <label for="nik" class="form-label">NIK</label>
                            <input type="text" class="form-control nik" id="nik" name="nik"  >
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md mb-3">
                            <label for="alamat" class="form-label">Alamat</label>
                            <div class="form-floating">
                                <textarea type="text" class="form-control alamat" id="alamat" name="alamat"  ></textarea>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md mb-3">
                            <label for="tahun_mulai" class="form-label">Tahun Mulai</label>
                            <input type="text" class="form-control tahun_mulai" id="tahun_mulai" name="tahun_mulai"  >
                        </div>
                        <div class="form-group col-md mb-3">
                            <label for="ukuran_baju" class="form-label">Ukuran Baju</label>
                            <input type="text" class="form-control ukuran_baju" id="ukuran_baju" name="ukuran_baju"  >
                        </div>
                        <div class="form-group col-md mb-3">
                            <label for="cabor" class="form-label">Cabor</label>
                            <select class="form-select cabor" id="cabor" name="cabor"  >
                                <option selected>Jenis Kelamin</option>
                                <option value="Laki-laki">Laki-laki</option>
                                <option value="Perempuan">Perempuan</option>
                            </select>
                        </div>
                    </div>
                    <div class="row">
                        <div class="form-group col-md mb-3">
                            <label for="asal_sekolah" class="form-label">Asal Sekolah</label>
                            <input type="text" class="form-control asal_sekolah" id="asal_sekolah" name="asal_sekolah"  >
                        </div>
                        <div class="form-group col-md mb-3">
                            <label for="fotoAtlet" class="form-label">Foto</label>
                            <input type="file" class="form-control fotoAtlet" id="fotoAtlet" name="fotoAtlet"  >
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <input type="submit" name="id_atlet" class="form-control product_id2" required />
                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                    <button type="submit" name="id_atlet" class="product_id2 btn btn-success">Edit</button>
                </div>
            </div>
        </div>
    </div>
</form>