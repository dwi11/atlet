<table id="tableCabor" class="table border" style="width:100%">
    <thead class="">
        <tr>
            <th>No</th>
            <th>Foto</th>
            <th>Profil</th>
            <th>Nama</th>
            <th>Aksi</th>
        </tr>
    </thead>
    <tbody>
            <% cabor.forEach(function(item, index) { %>
                <tr>
                  <td><%= index + 1 %></td>
                  <td>
                    <img src="<%= item.fotoCabor %>" class="img-thumbnail rounded" alt="Foto Atlet" width="70px">                
                  </td>
                  <td>
                    <%= item.nama %>
                  </td>
                  <td>
                    <%= item.sarana %>
                  </td>
                  <td>
                    <div class="btn-group">
                        <button href="javascript:void(0);" class="btn btn-lg btn-success edit"
                        data-id="<%= item.id_cabor %>" data-nama="<%= item.nama %>" data-sarana="<%= item.sarana %>" data-fotoCabor="<%= item.fotoCabor %>" 
                        >
                            <i class="fa-solid fa-pen-clip"></i>
                        </button>
                        <button href="javascript:void(0);" class="btn btn-lg btn-danger delete" type="button" data-id="<%= item.id_cabor %>">
                            <i class="fa-solid fa-delete-left"></i>
                        </button>
                    </div>
                </td>
                </tr>
              <% }); %>
    </tbody>
</table>

<script>
    $(document).ready(function () {
        $("#tableCabor").on("click", ".delete", function () {
            var id_cabor = $(this).data("id");
            $("#deleteCabor").modal("show");
            $(".product_id2").val(id_cabor);
        });
        $("#tableCabor").on("click", ".edit", function () {
        var id_cabor = $(this).data("id");
            var nama = $(this).data("nama");
            var sarana = $(this).data("sarana");
            var fotoCabor = $(this).data("fotoCabor");
            $("#editCabor").modal("show");
            $(".id_cabor").val(id_cabor);
            $(".nama").val(nama);
            $(".sarana").val(sarana);
            $(".fotoCabor").val(fotoCabor);
            $(".product_id2").val(id_cabor);
        });

    });
</script>